        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
	<connectionStrings>
		<add name="RedisConn"
			 connectionString="data source=localhost:6379;initial catalog=0"/>
	</connectionStrings>
